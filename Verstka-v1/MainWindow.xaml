<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Verstka_v1"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800">
    <Grid Background="blue">
        <TabControl>
            <TabItem Header="Grid"></TabItem>
            <TabItem Header="StackPanel"></TabItem>
            <TabItem Header="ComboBox" Height="22" Margin="-2,-2,-2,0" VerticalAlignment="Top"></TabItem>
            <TabControl Height="20"></TabControl>
        </TabControl>
    </Grid>
</Window>
